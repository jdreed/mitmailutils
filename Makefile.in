# $Id: Makefile.in,v 1.1 2004-06-28 19:34:34 rbasch Exp $

SHELL=/bin/sh
VPATH=@srcdir@
INSTALL=@INSTALL@
INSTALL_PROGRAM=@INSTALL_PROGRAM@
INSTALL_SCRIPT=@INSTALL_SCRIPT@
srcdir=@srcdir@
top_srcdir=@top_srcdir@
prefix=@prefix@
exec_prefix=@exec_prefix@
bindir=@bindir@
mandir=@mandir@

all:

check:

install:
	${top_srcdir}/mkinstalldirs ${DESTDIR}${bindir}
	${top_srcdir}/mkinstalldirs ${DESTDIR}${mandir}/man1
	${INSTALL_SCRIPT} mitmailcreate.pl ${DESTDIR}${bindir}/mitmailcreate
	ln -s mitmailcreate ${DESTDIR}${bindir}/mitmailremove
	ln -s mitmailcreate ${DESTDIR}${bindir}/mitmailsubscribe
	ln -s mitmailcreate ${DESTDIR}${bindir}/mitmailunsubscribe
	${INSTALL} -m 444 ${srcdir}/mitmailcreate.1 ${DESTDIR}${mandir}/man1
	ln -s mitmailcreate.1 ${DESTDIR}${mandir}/man1/mitmailremove.1
	ln -s mitmailcreate.1 ${DESTDIR}${mandir}/man1/mitmailsubscribe.1
	ln -s mitmailcreate.1 ${DESTDIR}${mandir}/man1/mitmailunsubscribe.1

clean:

distclean: clean
	rm -f config.cache config.log config.status Makefile
