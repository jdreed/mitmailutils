# $Id: Makefile.in,v 1.1 2004-09-03 20:44:42 rbasch Exp $

SHELL=/bin/sh
VPATH=@srcdir@
INSTALL=@INSTALL@
INSTALL_PROGRAM=@INSTALL_PROGRAM@
INSTALL_SCRIPT=@INSTALL_SCRIPT@
srcdir=@srcdir@
top_srcdir=@top_srcdir@
prefix=@prefix@
exec_prefix=@exec_prefix@
bindir=@bindir@
mandir=@mandir@

all:

check:

install:
	${top_srcdir}/mkinstalldirs ${DESTDIR}${bindir}
	${top_srcdir}/mkinstalldirs ${DESTDIR}${mandir}/man1
	set -xe; \
	for util in append create del exp move rename scan show; do \
	    ${INSTALL_SCRIPT} mitmail$$util.pl ${DESTDIR}${bindir}/mitmail$$util; \
	    ${INSTALL} -m 444 ${srcdir}/mitmail$$util.1 ${DESTDIR}${mandir}/man1; \
	done
	set -xe; \
	for util in quota usage; do \
	    ${INSTALL_SCRIPT} mail$$util.pl ${DESTDIR}${bindir}/mail$$util; \
	    ${INSTALL} -m 444 ${srcdir}/mail$$util.1 ${DESTDIR}${mandir}/man1; \
	done

clean:

distclean: clean
	rm -f config.cache config.log config.status Makefile
